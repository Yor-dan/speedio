{
  "manifest_version": 3,
  "name": "Speedio",
  "version": "1.0",
  "description": "Hold Space to speed up a video.",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "all_frames": true,
      "exclude_matches": [
        "*://www.youtube.com/*"
      ]
    }
  ]
}